{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/transform-runtime",
        [
            "@babel/plugin-transform-react-jsx",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "env": {
        "test": {
            "presets": [
                "es2015",
                "react"
            ],
            "compact": false
        }
    }
}